source 'https://rubygems.org'
# Bundle edge Rails instead: gem 'rails', github: 'rails/rails'
gem 'rails', '~> 5.1.4'
gem 'jquery-rails'
gem 'listen'
gem 'uglifier'
# Гем для вывода подробных логов на Heroku (с версии рельс 5.0.0 — не нужен)
gem 'rails_12factor'

group :production do
  gem 'pg'
end

group :development, :test do
  gem 'byebug'
end

group :development do
  gem 'sqlite3'
  gem 'web-console', '~> 2.0'
end

